a=raw_input("enter the car:")
class car:
    def __init__(self,rn,ecn):
         self.rn=rn
         self.ecn=ecn
    def cars(self):
         self.rn=input("enter the rn:")
         self.ecn=input("enter the ecn:")
         return self.rn,self.ecn
class bmw(car):
    def __init__(self):
         print "calling bmw"
class volvo(car):
    def __init__(self):
         print "calling volvo"
if a=="bmw":
    c=bmw()
    print c.cars()
elif a=="volvo":
    c=volvo()
    print c.cars()
else:
    print "no car found"
    
